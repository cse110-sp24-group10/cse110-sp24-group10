{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["source/calendar", "source/journal", "source/tasks"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/docdash"
  }
}
